#include "h_server_command_druga.h"

druga::druga(
		data *studentsAndClassroomData,
		pthread_mutex_t *data_thread,
		pthread_mutex_t *output_thread
	) : server_command(
		studentsAndClassroomData,
		data_thread,
		output_thread) {

};
bool druga::execute(int socket, char *message) {

	pthread_mutex_lock(output_thread);
	cout << "Bravo, bravo, radi ti i druga!" << endl;
	cout << "Primljena poruka je: " << message << endl;
	pthread_mutex_unlock(output_thread);
	
	return false;
};
